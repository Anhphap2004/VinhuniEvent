<style>
	/* Reset font to sans-serif */
	* {
		font-family: "Fredoka", "Poppins", sans-serif !important;
	}

	/* Tet Theme Variables */
	:root {
		--tet-red: #D32F2F;
		--tet-gold: #FDB913;
		--tet-red-light: #E8463D;
	}

	/* Winter Banner with Tet Theme */
	.winter-banner {
		position: relative;
		height: 620px;
		background: linear-gradient(135deg, var(--tet-red) 0%, #C8102E 50%, #8B0000 100%);
		overflow: hidden;
		display: flex;
		align-items: center;
		justify-content: center;
	}

	.winter-banner::before {
		content: '';
		position: absolute;
		width: 100%;
		height: 100%;
		background-image:
			radial-gradient(2px 2px at 20px 30px, rgba(253, 185, 19, 0.8), rgba(253, 185, 19, 0)),
			radial-gradient(2px 2px at 60px 70px, rgba(253, 185, 19, 0.8), rgba(253, 185, 19, 0)),
			radial-gradient(1px 1px at 50px 50px, rgba(255, 215, 0, 0.8), rgba(255, 215, 0, 0)),
			radial-gradient(1px 1px at 130px 80px, rgba(253, 185, 19, 0.8), rgba(253, 185, 19, 0)),
			radial-gradient(2px 2px at 90px 10px, rgba(255, 215, 0, 0.8), rgba(255, 215, 0, 0));
		background-repeat: repeat;
		background-size: 200px 200px;
		animation: snowfall 20s linear infinite;
		opacity: 0.7;
		z-index: 1;
	}

	@@keyframes snowfall {
		0% {
			transform: translateY(-100%);
		}

		100% {
			transform: translateY(100vh);
		}
	}

	.banner-content-winter {
		position: relative;
		z-index: 2;
		text-align: center;
		color: white;
		max-width: 800px;
		padding: 40px;
	}

	.banner-content-winter h1 {
		font-size: 56px;
		font-weight: 800;
		margin-bottom: 20px;
		line-height: 1.2;
		text-shadow: 0 6px 20px rgba(0, 0, 0, 0.4), 0 0 30px rgba(253, 185, 19, 0.3);
		animation: slideDown 0.8s ease-out;
		color: #FFF;
		letter-spacing: 2px;
	}

	.banner-content-winter .subtitle {
		font-size: 20px;
		margin-bottom: 30px;
		opacity: 0.95;
		animation: slideUp 0.8s ease-out 0.2s both;
	}

	.banner-stats-winter {
		display: flex;
		justify-content: center;
		gap: 60px;
		margin-bottom: 40px;
		flex-wrap: wrap;
		animation: slideUp 0.8s ease-out 0.4s both;
	}

	.stat-winter {
		text-align: center;
		background: rgba(255, 255, 255, 0.12);
		padding: 20px 30px;
		border-radius: 12px;
		backdrop-filter: blur(10px);
		border: 2px solid rgba(253, 185, 19, 0.3);
		box-shadow: 0 8px 32px rgba(211, 47, 47, 0.15);
	}

	.stat-number-winter {
		font-size: 40px;
		font-weight: 800;
		margin-bottom: 8px;
	}

	.stat-label-winter {
		font-size: 14px;
		text-transform: uppercase;
		letter-spacing: 1px;
		opacity: 0.9;
	}

	.banner-btn-winter {
		display: inline-block;
		background: linear-gradient(135deg, var(--tet-gold) 0%, #FFC107 100%);
		color: var(--tet-red);
		padding: 15px 45px;
		border-radius: 50px;
		text-decoration: none;
		font-weight: 800;
		font-size: 16px;
		text-transform: uppercase;
		letter-spacing: 1.5px;
		box-shadow: 0 12px 32px rgba(253, 185, 19, 0.4);
		transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1);
		animation: slideUp 0.8s ease-out 0.6s both;
	}

	.banner-btn-winter:hover {
		transform: translateY(-5px) scale(1.08);
		box-shadow: 0 18px 48px rgba(253, 185, 19, 0.6);
		color: #8B0000;
	}

	.snowflake-decoration {
		position: absolute;
		z-index: 2;
		font-size: 40px;
		opacity: 0.8;
		animation: float 6s ease-in-out infinite;
	}

	.snowflake-1 {
		top: 50px;
		left: 10%;
		animation-delay: 0s;
	}

	.snowflake-2 {
		top: 150px;
		right: 15%;
		animation-delay: 1s;
	}

	.snowflake-3 {
		bottom: 100px;
		left: 20%;
		animation-delay: 2s;
	}

	.snowflake-4 {
		top: 200px;
		right: 5%;
		animation-delay: 3s;
	}

	@@keyframes float {

		0%,
		100% {
			transform: translateY(0px) rotate(0deg);
		}

		50% {
			transform: translateY(-20px) rotate(180deg);
		}
	}

	@@keyframes slideDown {
		from {
			opacity: 0;
			transform: translateY(-30px);
		}

		to {
			opacity: 1;
			transform: translateY(0);
		}
	}

	@@keyframes slideUp {
		from {
			opacity: 0;
			transform: translateY(30px);
		}

		to {
			opacity: 1;
			transform: translateY(0);
		}
	}
</style>

