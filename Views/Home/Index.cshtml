<!-- Start featured-event Area -->
<section class="featured-event-area section-gap" id="featured-events">
	<style>
		.featured-event-area {
			background: linear-gradient(135deg, #FFF9E6 0%, #FFE8CC 50%, #FFF0D9 100%);
			position: relative;
			overflow: hidden;
		}

		.featured-event-area::before {
			content: '';
			position: absolute;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			background-image:
				radial-gradient(circle at 20% 50%, rgba(211, 47, 47, 0.08) 0%, transparent 50%),
				radial-gradient(circle at 80% 80%, rgba(253, 185, 19, 0.08) 0%, transparent 50%);
		}

		.featured-event-area .container {
			position: relative;
			z-index: 2;
		}

		.event-section-title {
			font-family: "Fredoka", sans-serif;
			text-align: center;
			margin-bottom: 35px;
		}

		.event-section-title h1 {
			font-size: 32px;
			font-weight: 800;
			background: linear-gradient(135deg, var(--tet-red) 0%, var(--tet-gold) 100%);
			-webkit-background-clip: text;
			-webkit-text-fill-color: transparent;
			background-clip: text;
			margin-bottom: 8px;
		}

		.event-section-title p {
			font-size: 14px;
			color: #888;
			max-width: 500px;
			margin: 0 auto;
			font-family: "Fredoka", sans-serif;
		}

		.events-list {
			display: grid;
			grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
			gap: 25px;
		}

		.event-item {
			background: white;
			border-radius: 12px;
			overflow: hidden;
			box-shadow: 0 4px 12px rgba(211, 47, 47, 0.08);
			transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1);
			display: flex;
			flex-direction: column;
			height: 100%;
			border: 2px solid transparent;
		}

		.event-item:hover {
			transform: translateY(-8px) scale(1.02);
			box-shadow: 0 12px 28px rgba(211, 47, 47, 0.18);
			border-color: var(--tet-gold);
		}

		.event-item-thumb {
			position: relative;
			width: 100%;
			height: 200px;
			overflow: hidden;
			background: linear-gradient(135deg, var(--tet-red) 0%, var(--tet-gold) 100%);
		}

		.event-item-thumb img {
			width: 100%;
			height: 100%;
			object-fit: cover;
			transition: transform 0.4s ease;
		}

		.event-item:hover .event-item-thumb img {
			transform: scale(1.08);
		}

		.event-item-content {
			display: flex;
			flex-direction: column;
			gap: 8px;
			padding: 18px;
			flex: 1;
		}

		.event-item-title {
			font-size: 16px;
			font-weight: 700;
			color: #2c3e50;
			margin: 0;
			line-height: 1.4;
			display: -webkit-box;
			-webkit-line-clamp: 2;
			-webkit-box-orient: vertical;
			overflow: hidden;
			font-family: "Fredoka", sans-serif;
		}

		.event-item-meta {
			display: flex;
			gap: 8px;
			align-items: center;
			flex-wrap: wrap;
			font-size: 12px;
		}

		.event-item-date {
			font-size: 12px;
			color: #7f8c8d;
			font-family: "Fredoka", sans-serif;
		}

		.event-item-category {
			display: inline-block;
			background: linear-gradient(135deg, rgba(211, 47, 47, 0.1), rgba(253, 185, 19, 0.08));
			color: var(--tet-red);
			padding: 4px 10px;
			border-radius: 20px;
			font-size: 11px;
			font-weight: 600;
			border: 1px solid var(--tet-gold);
			font-family: "Fredoka", sans-serif;
		}

		.event-item-btn {
			background: linear-gradient(135deg, var(--tet-red) 0%, var(--tet-gold) 100%);
			color: white;
			padding: 10px 16px;
			border-radius: 8px;
			text-decoration: none;
			font-size: 12px;
			font-weight: 700;
			text-transform: uppercase;
			letter-spacing: 0.5px;
			transition: all 0.3s cubic-bezier(0.34, 1.56, 0.64, 1);
			text-align: center;
			margin-top: auto;
			font-family: "Fredoka", sans-serif;
		}

		.event-item-btn:hover {
			transform: scale(1.05);
			box-shadow: 0 8px 20px rgba(211, 47, 47, 0.28);
			color: white;
		}

		.view-all-btn {
			display: flex;
			justify-content: center;
			margin-top: 35px;
		}

		.view-all-btn a {
			background: linear-gradient(135deg, var(--tet-red) 0%, var(--tet-gold) 100%);
			color: white;
			padding: 13px 40px;
			border-radius: 25px;
			text-decoration: none;
			font-weight: 800;
			font-size: 13px;
			text-transform: uppercase;
			letter-spacing: 1px;
			box-shadow: 0 8px 24px rgba(211, 47, 47, 0.25);
			transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1);
			font-family: "Fredoka", sans-serif;
		}

		.view-all-btn a:hover {
			transform: translateY(-4px) scale(1.06);
			box-shadow: 0 12px 36px rgba(211, 47, 47, 0.35);
		}

		@@media (max-width: 1024px) {
			.events-list {
				grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
				gap: 20px;
			}
		}

		@@media (max-width: 768px) {
			.events-list {
				grid-template-columns: repeat(2, 1fr);
				gap: 15px;
			}

			.event-item-thumb {
				height: 160px;
			}

			.event-item-content {
				padding: 14px;
				gap: 6px;
			}

			.event-item-title {
				font-size: 15px;
			}
		}

		@@media (max-width: 480px) {
			.events-list {
				grid-template-columns: 1fr;
			}

			.event-item-thumb {
				height: 180px;
			}

			.event-item-title {
				font-size: 14px;
			}
		}
	</style>

	<div class="container">
		<div class="event-section-title">
			<h1>🎊 Sự kiện nổi bật 🎊</h1>
			<p>Khám phá những khoảnh khắc rộn ràng của trường trong dịp Tết</p>
		</div>

		<div class="events-list">
			@foreach (var p in EventNew)
			{
				<div class="event-item">
					<div class="event-item-thumb">
						<img src="~/main/img/events/@p.Image" alt="@p.Title">
					</div>
					<div class="event-item-content">
						<h3 class="event-item-title">@p.Title</h3>
						<div class="event-item-meta">
							<span class="event-item-date">📅 @p.CreatedDate?.ToString("dd/MM/yyyy")</span>
							<span class="event-item-category">@p.Category?.CategoryName</span>
						</div>
					</div>
					<a href="@Url.Action("Details", "Events", new { slug = p.Slug, id = p.EventId })"
						class="event-item-btn">
						Xem →
					</a>
				</div>
			}
		</div>

		<div class="view-all-btn">
			<a href="@Url.Action("Index", "Events")">Xem tất cả sự kiện</a>
		</div>
	</div>
</section>
